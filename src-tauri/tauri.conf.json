{
	"build": {
		"beforeDevCommand": "pnpm dev",
		"beforeBuildCommand": "pnpm build",
		"devPath": "http://localhost:5173",
		"distDir": "../dist"
	},
	"package": {
		"productName": "SwiftLink",
		"version": "0.0.1"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"shell": {
				"all": false,
				"open": true
			}
		},
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "org.swiftlink",
			"icon": [
				"icons/icon.icns",
				"icons/icon.ico",
				"icons/128x128.png",
				"icons/StoreLogo.png",
				"icons/icon.svg",
				"icons/Square89x89Logo.png",
				"icons/Square71x71Logo.png",
				"icons/Square44x44Logo.png",
				"icons/Square310x310Logo.png",
				"icons/Square30x30Logo.png",
				"icons/Square284x284Logo.png",
				"icons/Square150x150Logo.png",
				"icons/Square142x142Logo.png",
				"icons/Square107x107Logo.png",
				"icons/icon.png",
				"icons/32x32.png",
				"icons/128x128@2x.png"
			],
			"externalBin": []
		},
		"security": {
			"csp": null
		},
		"windows": [
			{
				"fullscreen": false,
				"resizable": true,
				"title": "SwiftLink",
				"width": 800,
				"height": 600
			}
		]
	}
}
